q = int(input())
for _ in range(q):
    n = int(input())
    a = []
    for y in range(2*n):
        a.append([int(x) for x in input().split()])
    suma = 0
    for i in range(n):
        for j in range(n):
            suma += max(max(a[i][j],a[2*n-i-1][j]),max(a[i][2*n-j-1],a[2*n-i-1][2*n-j-1]))
    print(suma)
